#!/usr/bin/python3

class PMFFile:

    def __init__(self, filename):
        print("????")
        self.file = open(filename, 'bw')
        self.file.write(b'PMF')
        self.file.write(b'\0x01')

    def flush(self):
        self.file.flush()

if __name__ == "main":
    pmf = PMFFile("Test.pmf")
    pmf.flush()
